{
  "name": "react-expo-tabs-ts",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|@react-native-async-storage)"
    ]
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "tamagui", 
          "@tamagui/config", 
          "@tamagui/lucide-icons", 
          "form-data", 
          "prop-types", 
          "react-native-image-zoom-viewer"
        ]
      }
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-navigation/native": "^7.0.0",
    "@tamagui/config": "^1.116.15",
    "@tamagui/lucide-icons": "^1.116.15",
    "axios": "^1.6.7",
    "expo": "^52.0.0",
    "expo-camera": "~16.0.5",
    "expo-dev-client": "~5.0.2",
    "expo-file-system": "~18.0.3",
    "expo-font": "~13.0.1",
    "expo-image-picker": "~16.0.2",
    "expo-linking": "~7.0.2",
    "expo-location": "~18.0.1",
    "expo-media-library": "~17.0.2",
    "expo-router": "~4.0.6",
    "expo-secure-store": "~14.0.0",
    "expo-splash-screen": "~0.29.11",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.3",
    "expo-web-browser": "~14.0.1",
    "form-data": "^4.0.0",
    "prop-types": "^15.8.1",
    "react": "18.3.1",
    "react-content-loader": "^7.0.0",
    "react-dom": "18.3.1",
    "react-native": "0.76.2",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-maps": "1.18.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "^4.0.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.13",
    "tamagui": "^1.116.15"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@testing-library/react-native": "^12.5.1",
    "@types/react": "~18.3.12",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.2",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.3"
  },
  "private": true
}
